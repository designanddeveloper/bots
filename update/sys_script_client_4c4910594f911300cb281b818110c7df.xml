<?xml version="1.0" encoding="UTF-8"?><record_update sys_domain="global" table="sys_script_client">
    <sys_script_client action="INSERT_OR_UPDATE">
        <active>true</active>
        <applies_extended>false</applies_extended>
        <condition/>
        <description/>
        <field>u_quantity</field>
        <global>true</global>
        <isolate_script>false</isolate_script>
        <messages/>
        <name>Calculate Subtotal- quanity update</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading, isTemplate) {
	if (isLoading || newValue === '') {
		return;
	}
	var cost = g_form.getValue("u_price_cost");
	var perunit = 0;
	if (cost.indexOf(';')>0)
		{
		cost = cost.split(';');
		perunit = cost[1];
	}
	else
		{
		perunit = cost;
	}
	perunit = perunit.replace(/,/g,'');
	var itemtotal = perunit * g_form.getIntValue("u_quantity");
	itemtotal=itemtotal.toFixed(2);
	g_form.setValue("u_price", itemtotal);
	
}]]></script>
